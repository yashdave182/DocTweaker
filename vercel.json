{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/health.py": {
      "runtime": "vercel-python@3.9"
    },
    "api/enhance.py": {
      "runtime": "vercel-python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/enhance",
      "dest": "/api/enhance.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
